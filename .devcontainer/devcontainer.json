{
    "name": "control libraries",
    "remoteUser": "developer",
    "build": {
        "dockerfile": "../Dockerfile",
        "context": "..",
        "target": "development"
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/src,type=bind,consistency=cached",
    "workspaceFolder": "/src",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools-extension-pack"
            ]
        }
    }
}